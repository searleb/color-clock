{
  "manifest_version": 2,

  "name": "Color Clock",
  "description": "Color changing clock based on current time converted to hex color codes",
  "version": "0.1",
  "offline_enabled": true,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
     "128": "icon-128.png",
     "16": "icon-16.png",
     "48": "icon-48.png"
  }
}